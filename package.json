  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "1.2.0",
    "@types/jest": "^27.4.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "husky": "7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.15",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-jest": "^5.0.0-alpha.10",
